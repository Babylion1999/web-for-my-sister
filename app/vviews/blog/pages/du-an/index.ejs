<% const pathUpload="/backend/adminlte/images/product/"  %>

<section id="du-an-banner">
  <div class="page-tilte" > 
    
      <h3 class="text-center">DỰ ÁN</h3>
      
    </div>
  </section>
<section class="du-an">
    <div class="section-tittle">
        <div class="container text-center">
            <div class="border-tittle"></div>
            <h3>DỰ ÁN</h3>
            <h2>Các dự án đã hoàn thiện</h2>
        </div>
    </div>
    <div class="container">
      <div class="row">
       
        <% itemsProduct.forEach((item)=>{ %>
        <div class="col-md-6 col-lg-3" style="position: relative;">
          <a href="/du-an/<%= item.id %>">
            <div class="grid">
              <figure class="effect-lily"> <img src="<%= pathUpload + item.thumb0 %>" alt="img12">
                <figcaption>
                  <h4 style="font-size: 20px;"><a href="#"><%= item.title %></a></h4>
                  <p><%= item.description %></p>
                  <a href="tintuc.html">View more</a> </figcaption>
              </figure>
            </div>
          </a>
          <a href="javascript:likeNews('<%= item.id %>')" style="position: absolute; top:0; left:15%"  ><i class="fas fa-bookmark" id="like-<%= item.id %>" style="font-size: 40px;"></i></a>
        </div>
        <% })%>
          
          
      </div>
    </div>
          </section>